{
    "stryker-config": {
        "test-runner": "vstest",
        "reporters": [
            "progress",
            "html"
        ],
        "log-level": "info",
        "log-file": true,
        "timeout-ms": 10000,
        "project-file": "Calculadora/Calculadora.csproj",
        "max-concurrent-test-runners": 2,
        "threshold-high": 80,
        "threshold-low": 70,
        "threshold-break": 60,
        "file-patterns": [],
        "ignore-methods": [
            "*Log*",
            "ToString",
            "*HashCode*"
        ]
    }
}